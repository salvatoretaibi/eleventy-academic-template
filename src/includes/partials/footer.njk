<footer role="contentinfo">
  <div id="footer--container">
    <div id="footer--left">
      <a href="{{ site.https://italian.rutgers.edu/people/phd-students/115-deparmental-info/people/phd-graduate-students/458-taibi-salvatore }}">
        {{ site.name }} 
      </a>
    </div>
    <div id="footer--middle">
        {% svgsprite %}
          <a href="{{ site.twitterUrl }}" title="twitter">{% icon "twitter" %}</a>
          <a href="{{ site.githubUrl }}" title="github">{% icon "github" %}</a>
          <a href="{{ site.linkedInUrl }}" title="linkedin">{% icon "linkedin" %}</a>
          <a href="{{ site.instagramUrl }}" title="instagram">{% icon "instagram" %}</a>
    </div>
    <div id="footer--right"> 
      &copy; {{ site.copyright }}
    </div>
  </div>
</footer>